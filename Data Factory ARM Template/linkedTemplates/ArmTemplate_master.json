{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "apmorganworkspace"
        },
        "AzureDatabricks1_accessToken": {
            "type": "secureString",
            "metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
        },
        "storageEventTrigger_properties_datafilterpipeline_parameters_fileName": {
            "type": "string",
            "defaultValue": "@triggerBody().fileName"
        },
        "storageEventTrigger_properties_typeProperties_scope": {
            "type": "string",
            "defaultValue": "/subscriptions/c416a70b-1427-421f-bdfc-3d31f41f7430/resourceGroups/ap-morgan-project/providers/Microsoft.Storage/storageAccounts/casynapse256"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "apmorganworkspace_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "AzureDatabricks1_accessToken": {
                        "value": "[parameters('AzureDatabricks1_accessToken')]"
                    },
                    "storageEventTrigger_properties_datafilterpipeline_parameters_fileName": {
                        "value": "[parameters('storageEventTrigger_properties_datafilterpipeline_parameters_fileName')]"
                    },
                    "storageEventTrigger_properties_typeProperties_scope": {
                        "value": "[parameters('storageEventTrigger_properties_typeProperties_scope')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}